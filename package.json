{
  "name": "xslide",
  "productName": "Xslide",
  "version": "0.0.0",
  "description": "An application that can quickly view large images like .tiff, .svs and .mrxs file(s).",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "cd ./node_modules/.bin && electron-rebuild -f --module-dir=../../",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "ffi": "lxe/node-ffi#node-12",
    "ref": "lxe/ref#node-12",
    "ref-struct": "lxe/ref-struct#node-12",
    "ref-array": "lxe/ref-array#node-12",
    "weak": "lxe/node-weak#node-12"
  },
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-rebuild": "^1.8.4"
  },
  "keywords": [
    "tiff",
    "svs",
    "mrxs",
    "openslide",
    "vips",
    "viewer"
  ],
  "author": "Hurry",
  "license": "MIT"
}